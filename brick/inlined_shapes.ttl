@prefix brick: <https://brickschema.org/schema/Brick#> .
@prefix obc: <urn:hpflex/shapes#> .
@prefix qudt: <http://qudt.org/schema/qudt/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ref: <https://brickschema.org/schema/Brick/ref#> .
@prefix s223: <http://data.ashrae.org/standard223#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

obc:cooset a rdfs:Class,
        sh:NodeShape ;
    sh:class brick:Zone_Air_Cooling_Temperature_Setpoint ;
    sh:property [ a sh:PropertyShape ;
            sh:path ref:hasExternalReference ],
        [ a sh:PropertyShape ;
            sh:minCount 2 ;
            sh:path ref:hasExternalReference ],
        [ a sh:PropertyShape ;
            sh:hasValue qudt:K ;
            sh:minCount 1 ;
            sh:path qudt:hasUnit ] .

obc:cooset_u a rdfs:Class,
        sh:NodeShape ;
    sh:class s223:FunctionInput ;
    sh:property [ a sh:PropertyShape ;
            sh:minCount 1 ;
            sh:path s223:uses ] .

obc:heaset a rdfs:Class,
        sh:NodeShape ;
    sh:class brick:Zone_Air_Heating_Temperature_Setpoint ;
    sh:property [ a sh:PropertyShape ;
            sh:hasValue qudt:K ;
            sh:minCount 1 ;
            sh:path qudt:hasUnit ],
        [ a sh:PropertyShape ;
            sh:path ref:hasExternalReference ],
        [ a sh:PropertyShape ;
            sh:minCount 2 ;
            sh:path ref:hasExternalReference ] .

obc:heaset_u a rdfs:Class,
        sh:NodeShape ;
    sh:class s223:FunctionInput ;
    sh:property [ a sh:PropertyShape ;
            sh:minCount 1 ;
            sh:path s223:uses ] .

obc:normal-ref a rdfs:Class,
        sh:NodeShape ;
    sh:class ref:NormalReference .

obc:occ a rdfs:Class,
        sh:NodeShape ;
    sh:class brick:Occupancy_Sensor ;
    sh:property [ a sh:PropertyShape ;
            sh:minCount 2 ;
            sh:path ref:hasExternalReference ],
        [ a sh:PropertyShape ;
            sh:path ref:hasExternalReference ] .

obc:ref a rdfs:Class,
        sh:NodeShape ;
    sh:class ref:BOPTestReference .

obc:temp a rdfs:Class,
        sh:NodeShape ;
    sh:class brick:Zone_Air_Temperature_Sensor ;
    sh:property [ a sh:PropertyShape ;
            sh:hasValue qudt:K ;
            sh:minCount 1 ;
            sh:path qudt:hasUnit ],
        [ a sh:PropertyShape ;
            sh:minCount 2 ;
            sh:path ref:hasExternalReference ],
        [ a sh:PropertyShape ;
            sh:path ref:hasExternalReference ] .

obc:temp_u a rdfs:Class,
        sh:NodeShape ;
    sh:class s223:FunctionInput ;
    sh:property [ a sh:PropertyShape ;
            sh:minCount 1 ;
            sh:path s223:uses ] .

obc:temp_y a rdfs:Class,
        sh:NodeShape ;
    sh:class s223:FunctionOutput ;
    sh:property [ a sh:PropertyShape ;
            sh:minCount 1 ;
            sh:path s223:uses ] .

obc:zone a rdfs:Class,
        sh:NodeShape ;
    sh:class brick:Zone ;
    sh:property [ a sh:PropertyShape ;
            sh:path brick:hasPoint ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ a sh:NodeShape ;
                    rdfs:label "zone_cooset-1" ;
                    sh:class obc:cooset ] ],
        [ a sh:PropertyShape ;
            sh:path brick:hasPoint ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ a sh:NodeShape ;
                    rdfs:label "zone_temp-1" ;
                    sh:class obc:temp ] ],
        [ a sh:PropertyShape ;
            sh:minCount 4 ;
            sh:path brick:hasPoint ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ a sh:NodeShape ;
                    rdfs:label "zone_heaset-1" ;
                    sh:class obc:heaset ] ],
        [ a sh:PropertyShape ;
            sh:path brick:hasPoint ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ a sh:NodeShape ;
                    rdfs:label "zone_occ-1" ;
                    sh:class obc:occ ] ] .

obc:zone-temp-controller a rdfs:Class,
        sh:NodeShape ;
    sh:class s223:Function ;
    sh:property [ a sh:PropertyShape ;
            sh:minCount 3 ;
            sh:path s223:hasInput ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ a sh:NodeShape ;
                    rdfs:label "zone-temp-controller_temp_u-1" ;
                    sh:class obc:temp_u ] ],
        [ a sh:PropertyShape ;
            sh:path s223:hasInput ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ a sh:NodeShape ;
                    rdfs:label "zone-temp-controller_heaset_u-1" ;
                    sh:class obc:heaset_u ] ],
        [ a sh:PropertyShape ;
            sh:minCount 1 ;
            sh:path s223:hasOutput ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ a sh:NodeShape ;
                    rdfs:label "zone-temp-controller_temp_y-1" ;
                    sh:class obc:temp_y ] ],
        [ a sh:PropertyShape ;
            sh:path s223:hasInput ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ a sh:NodeShape ;
                    rdfs:label "zone-temp-controller_cooset_u-1" ;
                    sh:class obc:cooset_u ] ] .

obc:zone2 a rdfs:Class,
        sh:NodeShape ;
    sh:class brick:Zone ;
    sh:property [ a sh:PropertyShape ;
            sh:path brick:hasPoint ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ a sh:NodeShape ;
                    rdfs:label "zone2_temp-1" ;
                    sh:class obc:temp ] ],
        [ a sh:PropertyShape ;
            sh:minCount 3 ;
            sh:path brick:hasPoint ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ a sh:NodeShape ;
                    rdfs:label "zone2_heaset-1" ;
                    sh:class obc:heaset ] ],
        [ a sh:PropertyShape ;
            sh:path brick:hasPoint ;
            sh:qualifiedMinCount 1 ;
            sh:qualifiedValueShape [ a sh:NodeShape ;
                    rdfs:label "zone2_cooset-1" ;
                    sh:class obc:cooset ] ] .

obc:coosetAnnotation a sh:NodeShape ;
    sh:rule [ a sh:TripleRule ;
            sh:condition obc:cooset ;
            sh:object obc:cooset ;
            sh:predicate rdf:type ;
            sh:subject sh:this ] ;
    sh:targetClass brick:Zone_Air_Cooling_Temperature_Setpoint .

obc:cooset_uAnnotation a sh:NodeShape ;
    sh:rule [ a sh:TripleRule ;
            sh:condition obc:cooset_u ;
            sh:object obc:cooset_u ;
            sh:predicate rdf:type ;
            sh:subject sh:this ] ;
    sh:targetClass s223:FunctionInput .

obc:has-pointAnnotation a sh:NodeShape ;
    sh:rule [ a sh:SPARQLRule ;
            sh:construct """
                CONSTRUCT {
                    ?name <urn:hpflex/shapes#has-point> ?target .
                }
                WHERE {
	?name <https://brickschema.org/schema/Brick#hasPoint> ?target . 
}""" ] ;
    sh:targetSubjectsOf brick:hasPoint .

obc:heasetAnnotation a sh:NodeShape ;
    sh:rule [ a sh:TripleRule ;
            sh:condition obc:heaset ;
            sh:object obc:heaset ;
            sh:predicate rdf:type ;
            sh:subject sh:this ] ;
    sh:targetClass brick:Zone_Air_Heating_Temperature_Setpoint .

obc:heaset_uAnnotation a sh:NodeShape ;
    sh:rule [ a sh:TripleRule ;
            sh:condition obc:heaset_u ;
            sh:object obc:heaset_u ;
            sh:predicate rdf:type ;
            sh:subject sh:this ] ;
    sh:targetClass s223:FunctionInput .

obc:normal-refAnnotation a sh:NodeShape ;
    sh:rule [ a sh:TripleRule ;
            sh:condition obc:normal-ref ;
            sh:object obc:normal-ref ;
            sh:predicate rdf:type ;
            sh:subject sh:this ] ;
    sh:targetClass ref:NormalReference .

obc:occAnnotation a sh:NodeShape ;
    sh:rule [ a sh:TripleRule ;
            sh:condition obc:occ ;
            sh:object obc:occ ;
            sh:predicate rdf:type ;
            sh:subject sh:this ] ;
    sh:targetClass brick:Occupancy_Sensor .

obc:refAnnotation a sh:NodeShape ;
    sh:rule [ a sh:TripleRule ;
            sh:condition obc:ref ;
            sh:object obc:ref ;
            sh:predicate rdf:type ;
            sh:subject sh:this ] ;
    sh:targetClass ref:BOPTestReference .

obc:tempAnnotation a sh:NodeShape ;
    sh:rule [ a sh:TripleRule ;
            sh:condition obc:temp ;
            sh:object obc:temp ;
            sh:predicate rdf:type ;
            sh:subject sh:this ] ;
    sh:targetClass brick:Zone_Air_Temperature_Sensor .

obc:temp_uAnnotation a sh:NodeShape ;
    sh:rule [ a sh:TripleRule ;
            sh:condition obc:temp_u ;
            sh:object obc:temp_u ;
            sh:predicate rdf:type ;
            sh:subject sh:this ] ;
    sh:targetClass s223:FunctionInput .

obc:temp_yAnnotation a sh:NodeShape ;
    sh:rule [ a sh:TripleRule ;
            sh:condition obc:temp_y ;
            sh:object obc:temp_y ;
            sh:predicate rdf:type ;
            sh:subject sh:this ] ;
    sh:targetClass s223:FunctionOutput .

obc:zone-temp-controllerAnnotation a sh:NodeShape ;
    sh:rule [ a sh:TripleRule ;
            sh:condition obc:zone-temp-controller ;
            sh:object obc:zone-temp-controller ;
            sh:predicate rdf:type ;
            sh:subject sh:this ] ;
    sh:targetClass s223:Function .

obc:zone2Annotation a sh:NodeShape ;
    sh:rule [ a sh:TripleRule ;
            sh:condition obc:zone2 ;
            sh:object obc:zone2 ;
            sh:predicate rdf:type ;
            sh:subject sh:this ] ;
    sh:targetClass brick:Zone .

obc:zoneAnnotation a sh:NodeShape ;
    sh:rule [ a sh:TripleRule ;
            sh:condition obc:zone ;
            sh:object obc:zone ;
            sh:predicate rdf:type ;
            sh:subject sh:this ] ;
    sh:targetClass brick:Zone .


ctrl:zone_ratcheting obc:controls obc:zone ;
    obc:uses ctrl:TZonHeaSetCur, ctrl:TZonCooSetCur,
        ctrl:TZonHeaSetCom, ctrl:TZonCooSetCom, ctrl:TZon .
    
ctrl:TZonHeaSetCur a s223:FunctionInput;
    obc:binds obc:heaset .

ctrl:TZonCooSetCur a s223:FunctionInput;
    obc:binds obc:cooset .

ctrl:TZon a s223:FunctionInput;
    obc:binds obc:temp .