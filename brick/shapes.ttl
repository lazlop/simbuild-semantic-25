@prefix brick: <https://brickschema.org/schema/Brick#> .
@prefix hpfs: <urn:hpflex/shapes#> .
@prefix qudt: <http://qudt.org/schema/qudt/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ref: <https://brickschema.org/schema/Brick/ref#> .
@prefix s223: <http://data.ashrae.org/standard223#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

hpfs:cooset a rdfs:Class,
        sh:NodeShape ;
    sh:class brick:Zone_Air_Cooling_Temperature_Setpoint ;
    sh:property hpfs:cooset_K,
        hpfs:cooset_ref .

hpfs:cooset_u a rdfs:Class,
        sh:NodeShape ;
    sh:class s223:FunctionInput ;
    sh:property hpfs:cooset_u_cooset .

hpfs:heaset a rdfs:Class,
        sh:NodeShape ;
    sh:class brick:Zone_Air_Heating_Temperature_Setpoint ;
    sh:property hpfs:heaset_K,
        hpfs:heaset_ref .

hpfs:heaset_u a rdfs:Class,
        sh:NodeShape ;
    sh:class s223:FunctionInput ;
    sh:property hpfs:heaset_u_heaset .

hpfs:ref a rdfs:Class,
        sh:NodeShape ;
    sh:class ref:BOPTestReference .

hpfs:temp a rdfs:Class,
        sh:NodeShape ;
    sh:class brick:Zone_Air_Temperature_Sensor ;
    sh:property hpfs:temp_K,
        hpfs:temp_ref .

hpfs:temp_u a rdfs:Class,
        sh:NodeShape ;
    sh:class s223:FunctionInput ;
    sh:property hpfs:temp_u_temp .

hpfs:temp_y a rdfs:Class,
        sh:NodeShape ;
    sh:class s223:FunctionOutput ;
    sh:property hpfs:temp_y_temp .

hpfs:zone a rdfs:Class,
        sh:NodeShape ;
    sh:class brick:Zone ;
    sh:property hpfs:zone_cooset,
        hpfs:zone_heaset,
        hpfs:zone_temp .

hpfs:zone-temp-controller a rdfs:Class,
        sh:NodeShape ;
    sh:class s223:Function ;
    sh:property hpfs:zone-temp-controller_cooset_u,
        hpfs:zone-temp-controller_heaset_u,
        hpfs:zone-temp-controller_temp_u,
        hpfs:zone-temp-controller_temp_y .

hpfs:coosetAnnotation a sh:NodeShape ;
    sh:rule hpfs:coosetAnnotationRule ;
    sh:targetClass brick:Zone_Air_Cooling_Temperature_Setpoint .

hpfs:cooset_uAnnotation a sh:NodeShape ;
    sh:rule hpfs:cooset_uAnnotationRule ;
    sh:targetClass s223:FunctionInput .

hpfs:has-pointAnnotation a sh:NodeShape ;
    sh:rule hpfs:has-pointAnnotationRule ;
    sh:targetSubjectsOf brick:hasPoint .

hpfs:heasetAnnotation a sh:NodeShape ;
    sh:rule hpfs:heasetAnnotationRule ;
    sh:targetClass brick:Zone_Air_Heating_Temperature_Setpoint .

hpfs:heaset_uAnnotation a sh:NodeShape ;
    sh:rule hpfs:heaset_uAnnotationRule ;
    sh:targetClass s223:FunctionInput .

hpfs:refAnnotation a sh:NodeShape ;
    sh:rule hpfs:refAnnotationRule ;
    sh:targetClass ref:BOPTestReference .

hpfs:tempAnnotation a sh:NodeShape ;
    sh:rule hpfs:tempAnnotationRule ;
    sh:targetClass brick:Zone_Air_Temperature_Sensor .

hpfs:temp_uAnnotation a sh:NodeShape ;
    sh:rule hpfs:temp_uAnnotationRule ;
    sh:targetClass s223:FunctionInput .

hpfs:temp_yAnnotation a sh:NodeShape ;
    sh:rule hpfs:temp_yAnnotationRule ;
    sh:targetClass s223:FunctionOutput .

hpfs:zone-temp-controllerAnnotation a sh:NodeShape ;
    sh:rule hpfs:zone-temp-controllerAnnotationRule ;
    sh:targetClass s223:Function .

hpfs:zoneAnnotation a sh:NodeShape ;
    sh:rule hpfs:zoneAnnotationRule ;
    sh:targetClass brick:Zone .

hpfs:coosetAnnotationRule a sh:TripleRule ;
    sh:condition hpfs:cooset ;
    sh:object hpfs:cooset ;
    sh:predicate rdf:type ;
    sh:subject sh:this .

hpfs:cooset_K a sh:PropertyShape ;
    sh:hasValue qudt:K ;
    sh:minCount 1 ;
    sh:path qudt:hasUnit .

hpfs:cooset_ref a sh:PropertyShape ;
    sh:minCount 1 ;
    sh:path ref:hasExternalReference .

hpfs:cooset_uAnnotationRule a sh:TripleRule ;
    sh:condition hpfs:cooset_u ;
    sh:object hpfs:cooset_u ;
    sh:predicate rdf:type ;
    sh:subject sh:this .

hpfs:cooset_u_cooset a sh:PropertyShape ;
    sh:minCount 1 ;
    sh:path s223:uses .

hpfs:has-pointAnnotationRule a sh:SPARQLRule ;
    sh:construct """
                CONSTRUCT {
                    ?name <urn:hpflex/shapes#has-point> ?target .
                }
                WHERE {
	?name <https://brickschema.org/schema/Brick#hasPoint> ?target . 
}""" .

hpfs:heasetAnnotationRule a sh:TripleRule ;
    sh:condition hpfs:heaset ;
    sh:object hpfs:heaset ;
    sh:predicate rdf:type ;
    sh:subject sh:this .

hpfs:heaset_K a sh:PropertyShape ;
    sh:hasValue qudt:K ;
    sh:minCount 1 ;
    sh:path qudt:hasUnit .

hpfs:heaset_ref a sh:PropertyShape ;
    sh:minCount 1 ;
    sh:path ref:hasExternalReference .

hpfs:heaset_uAnnotationRule a sh:TripleRule ;
    sh:condition hpfs:heaset_u ;
    sh:object hpfs:heaset_u ;
    sh:predicate rdf:type ;
    sh:subject sh:this .

hpfs:heaset_u_heaset a sh:PropertyShape ;
    sh:minCount 1 ;
    sh:path s223:uses .

hpfs:refAnnotationRule a sh:TripleRule ;
    sh:condition hpfs:ref ;
    sh:object hpfs:ref ;
    sh:predicate rdf:type ;
    sh:subject sh:this .

hpfs:tempAnnotationRule a sh:TripleRule ;
    sh:condition hpfs:temp ;
    sh:object hpfs:temp ;
    sh:predicate rdf:type ;
    sh:subject sh:this .

hpfs:temp_K a sh:PropertyShape ;
    sh:hasValue qudt:K ;
    sh:minCount 1 ;
    sh:path qudt:hasUnit .

hpfs:temp_ref a sh:PropertyShape ;
    sh:minCount 1 ;
    sh:path ref:hasExternalReference .

hpfs:temp_uAnnotationRule a sh:TripleRule ;
    sh:condition hpfs:temp_u ;
    sh:object hpfs:temp_u ;
    sh:predicate rdf:type ;
    sh:subject sh:this .

hpfs:temp_u_temp a sh:PropertyShape ;
    sh:minCount 1 ;
    sh:path s223:uses .

hpfs:temp_yAnnotationRule a sh:TripleRule ;
    sh:condition hpfs:temp_y ;
    sh:object hpfs:temp_y ;
    sh:predicate rdf:type ;
    sh:subject sh:this .

hpfs:temp_y_temp a sh:PropertyShape ;
    sh:minCount 1 ;
    sh:path s223:uses .

hpfs:zone-temp-controllerAnnotationRule a sh:TripleRule ;
    sh:condition hpfs:zone-temp-controller ;
    sh:object hpfs:zone-temp-controller ;
    sh:predicate rdf:type ;
    sh:subject sh:this .

hpfs:zone-temp-controller_cooset_u a sh:PropertyShape ;
    sh:path s223:hasInput ;
    sh:qualifiedMinCount 1 ;
    sh:qualifiedValueShape hpfs:zone-temp-controller_cooset_u-1 .

hpfs:zone-temp-controller_cooset_u-1 a sh:NodeShape ;
    rdfs:label "zone-temp-controller_cooset_u-1" ;
    sh:class hpfs:cooset_u .

hpfs:zone-temp-controller_heaset_u a sh:PropertyShape ;
    sh:minCount 3 ;
    sh:path s223:hasInput ;
    sh:qualifiedMinCount 1 ;
    sh:qualifiedValueShape hpfs:zone-temp-controller_heaset_u-1 .

hpfs:zone-temp-controller_heaset_u-1 a sh:NodeShape ;
    rdfs:label "zone-temp-controller_heaset_u-1" ;
    sh:class hpfs:heaset_u .

hpfs:zone-temp-controller_temp_u a sh:PropertyShape ;
    sh:path s223:hasInput ;
    sh:qualifiedMinCount 1 ;
    sh:qualifiedValueShape hpfs:zone-temp-controller_temp_u-1 .

hpfs:zone-temp-controller_temp_u-1 a sh:NodeShape ;
    rdfs:label "zone-temp-controller_temp_u-1" ;
    sh:class hpfs:temp_u .

hpfs:zone-temp-controller_temp_y a sh:PropertyShape ;
    sh:minCount 1 ;
    sh:path s223:hasOutput ;
    sh:qualifiedMinCount 1 ;
    sh:qualifiedValueShape hpfs:zone-temp-controller_temp_y-1 .

hpfs:zone-temp-controller_temp_y-1 a sh:NodeShape ;
    rdfs:label "zone-temp-controller_temp_y-1" ;
    sh:class hpfs:temp_y .

hpfs:zoneAnnotationRule a sh:TripleRule ;
    sh:condition hpfs:zone ;
    sh:object hpfs:zone ;
    sh:predicate rdf:type ;
    sh:subject sh:this .

hpfs:zone_cooset a sh:PropertyShape ;
    sh:minCount 3 ;
    sh:path brick:hasPoint ;
    sh:qualifiedMinCount 1 ;
    sh:qualifiedValueShape hpfs:zone_cooset-1 .

hpfs:zone_cooset-1 a sh:NodeShape ;
    rdfs:label "zone_cooset-1" ;
    sh:class hpfs:cooset .

hpfs:zone_heaset a sh:PropertyShape ;
    sh:path brick:hasPoint ;
    sh:qualifiedMinCount 1 ;
    sh:qualifiedValueShape hpfs:zone_heaset-1 .

hpfs:zone_heaset-1 a sh:NodeShape ;
    rdfs:label "zone_heaset-1" ;
    sh:class hpfs:heaset .

hpfs:zone_temp a sh:PropertyShape ;
    sh:path brick:hasPoint ;
    sh:qualifiedMinCount 1 ;
    sh:qualifiedValueShape hpfs:zone_temp-1 .

hpfs:zone_temp-1 a sh:NodeShape ;
    rdfs:label "zone_temp-1" ;
    sh:class hpfs:temp .

